{
  "name": "faker",
  "description": "Generate fake data from people.",
  "version": "1.0.0",
  "contributors": [
    "Malves <malves.dev@gmail.com>",
    "Marak Squires <marak.squires@gmail.com>"
  ],
  "repository": {
    "type": "git",
    "url": "http://github.com/malves.dev/Faker.js.git"
  },
  "scripts": {
    "lint": "node_modules/.bin/jshint ./lib --config ./.jshintrc",
    "test": "node_modules/.bin/mocha test/*.*.js",
    "build": "cd build && ../node_modules/.bin/gulp && cd ../",
    "doc": "jsdoc -c conf.json -t ./node_modules/ink-docstrap/template -R README.md lib",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "nyc": {
    "all": false,
    "include": [
      "lib/**/*.js",
      "vendor/*.js"
    ],
    "exclude": [
      "coverage",
      "locales",
      "modules",
      "white",
      "docs",
      "docker",
      "public",
      "reports",
      "tests",
      "node_modules"
    ],
    "reporter": [
      "html",
      "lcov",
      "clover"
    ],
    "report-dir": "./reports/coverage"
  },
  "devDependencies": {
    "browserify": "^16.2.3",
    "coveralls": "^3.0.3",
    "gulp": "^4.0.1",
    "gulp-gh-pages": "^0.5.4",
    "gulp-jsdoc3": "^0.2.1",
    "gulp-mustache": "^4.1.1",
    "gulp-rename": "1.2.0",
    "gulp-uglify": "^3.0.2",
    "ink-docstrap": "1.1.4",
    "jsdoc": "^3.6.0",
    "jshint": "^2.10.2",
    "lodash": "^4.6.1",
    "mocha": "^6.1.4",
    "node-minify": "*",
    "optimist": "0.3.5",
    "sinon": "1.4.2",
    "through2": "2.0.0",
    "vinyl-transform": "^1.0.0"
  },
  "license": "MIT",
  "main": "index.js",
  "dependencies": {
    "g": "^2.0.1"
  }
}
